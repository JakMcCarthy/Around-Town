<div class="myNav">
    <div>
        <a href="/">Main</a>
    </div>
    <div>
        <% if (loggedIn) { %>
            <a href="/add-comment">Add A Comment</a>
            <% } %>
            <a href="/display-comments">Display Comments</a>
    </div>
    <div>
        <% if (!loggedIn) { %>
